<%= simple_form_for(@order) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :feedback_comment %>
    <%= f.input :feedback_rating %>
    <%= f.input :buyer_id %>
    <%= f.input :seller_id %>
    <%= f.input :item_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
